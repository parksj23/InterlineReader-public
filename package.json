    "compression": "^1.7.3",
    "connect-flash": "~0.1.1",
    "cross-env": "3.1.3",
    "express": "^4.16.3",
    "express-session": "^1.0.0",
    "loadable-components": "0.4.0",
    "mongoose": "4.4",
    "react": "^16.4.2",
    "react-context-component": "0.0.6",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
