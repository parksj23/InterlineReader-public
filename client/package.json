    "jwt-decode": "^2.2.0",
    "moment": "^2.22.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.2",
    "react-content-loader": "^3.4.1",
    "react-dom": "^16.3.2",
    "react-fuse-picker": "^1.0.0",
    "react-highlighter": "^0.4.3",
    "react-html-parser": "^2.0.2",
    "react-moment": "^0.7.0",
    "react-redux": "^5.0.7",
